
public class Tester {

public static void chooseRoom (String newAnimal, boolean sick)
{
	if(newAnimal.equals("dog") || (newAnimal.equals("Dog"))
	{
		if (sick == false)
		{
			System.out.println("Goes in dog Room");
		}
		else
		{
			System.out.println("Does no accept");
		}
	}
	else if(newAnimal.equals("cat") || newAnimal.equals("Cat"))
	{
		if (sick == false)
		{
			System.out.println("Goes in cat Room");
		}
		else
		{
			System.out.println("Does no accept");
		}
	}
	else if(newAnimal.equals("zebra") || newAnimal.equals("Zebra"))
	{
		if (sick == false)
		{
			System.out.println("Goes in zebra Room");
		}
		else
		{
			System.out.println("Does no accept");
		}
	}
	else 
	{
		if (sick == false)
		{
			System.out.println("Goes in other Room");
		}
		else
		{
			System.out.println("Does no accept");
		}
	}
	}
	
	
