	public static double getPrice(String product)
	{
		
		if(product.equals("MacBook"))
		{
			return 1000.99;
		}
		else if(product.equals("IPhone"))
		{
			return  899.9;
		}
		else if(product.equals("Charger"))
		{
			return  19.99;
		}
		else if(product.equals("IPad"))
		{
			return  699.99;
		}
		else if(product.equals("TV"))
		{
			return  2000.99;
		}
		else 
		{
			return  0;
		}
		
		}
		
	}

